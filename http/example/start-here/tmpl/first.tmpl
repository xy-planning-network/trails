{{ define "first" }}
  <div class="template-name">-- first.tmpl --</div>
  <h1>Hello, from main!</h1>
  {{ block "next" . }}<div>I am next!</div>{{ end }}
  {{ block "last" . }}<div>I am the last bit of content.</div>{{ end }}
  <table>
  {{ range $k, $v := .Data }}
  <tr><td><strong>{{ $k }}</strong></td><td>{{ $v }}</td></tr>
  {{ end }}
  </table>
{{ end }}
